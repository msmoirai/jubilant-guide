
public class Loops {
	public static void main(String[] args) {
	
		//whileLoop();
		doWhileLoop();
		}

	
	static void whileLoop() {
		//while loop
		int counter = 9;
		while(counter < 10) {
			counter = counter +1;
			System.out.println(counter);
		}
	}
	
	static void doWhileLoop() {
		int counter = 10;
		do {
			counter = counter +1;
			System.out.println(counter);
		} while(counter < 10);
	}
}
