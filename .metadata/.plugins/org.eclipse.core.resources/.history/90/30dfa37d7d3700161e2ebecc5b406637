
public class Conditionals {
	public static void main(String[] args) {
		
		// if (some expression that evalute to true)
		// 	do something
		
		boolean sayHello = true;
		boolean sayGoodbye = false;
		
		if(sayHello) {
			System.out.println("Hello");
		}
		
		if (sayGoodbye) {
			System.out.println("Goodbye");
		}
	}
}
